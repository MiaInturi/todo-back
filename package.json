  "description": "Todo application backend",
  "scripts": {
    "build": "tsc && tsc-alias",
    "production": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec npm run start",
    "start": "ts-node -r tsconfig-paths/register --files src/index.ts",
    "lint": "eslint src/**/*.ts"
