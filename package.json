    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/uuid": "^8.3.4",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "i18next": "^21.6.14",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-middleware": "^3.2.0",
